pipeline{
  agent any
  stages {
   stage('build'){
	  steps{
		sh 'echo "Build"'	  
	  }
    }
   stage('test'){
	  steps{
		sh 'echo "Test"'	  
	  }
    }
   stage('development'){
	  steps{
		sh 'echo "Develop"'	  
	  }
    }
   stage('production'){
	  steps{
		sh 'echo "Production"'	  
	  }
    }
  }
}
